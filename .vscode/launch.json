    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'circom'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=circom",
                ],
                "filter": {
                    "name": "circom",
                    "kind": "bin"
                }
            },
            "args": ["${workspaceFolder}/src/assets/circuit.circom"],
            "cwd": "${workspaceFolder}"
        },
    ]
}